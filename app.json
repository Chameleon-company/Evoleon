{
  "expo": {
    "name": "Evoleon",
    "slug": "evoleon",
    "version": "0.2.4\nSplash of Colour Update",
    "orientation": "portrait",
    "icon": "./assets/EvoleonLandingTemp.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/EvoleonLandingTemp.png",
      "resizeMode": "contain",
      "backgroundColor": "#F7F7FF",
      "imageResizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "description": "Evoleon is a mobile app that allows you to search for an electric charger near you, base on a criteria entered by you. It also allows you to add a charger to the database, so that other users can find it. ",
    "platforms": ["ios", "android"],
    "android": {
      "package": "com.evoleonapp.evoleon"
    }
  }
}
